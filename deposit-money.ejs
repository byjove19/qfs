    <script>
        async function copyToClipboard(elementId) {
            try {
                const cryptoAddressElement = document.getElementById(elementId);
                if (!cryptoAddressElement) {
                    console.error(`Element with ID "${elementId}" not found.`);
                    return;
                }

                const textToCopy = cryptoAddressElement.value;
                await navigator.clipboard.writeText(textToCopy);
                alert(`Address copied to clipboard: ${textToCopy}`);
            } catch (err) {
                console.error('Failed to copy text: ', err);
                alert('Failed to copy address to clipboard. Please try again.');
            }
        }

